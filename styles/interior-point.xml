<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map>
<Map background-color="white" srs="+proj=latlong +datum=WGS84">
    <Parameters>
        <Parameter name="sizes">256, 256</Parameter>
    </Parameters>

    <Style name="test">
        <Rule>
            <Filter>[mapnik::geometry_type] = linestring</Filter>
            <LineSymbolizer />
            <TextSymbolizer
                face-name="DejaVu Sans Book"
                placement="line"
                size="10"
                halo-radius="1"
                fill="purple"
                allow-overlap="true"
                minimum-path-length="200"
                >
                "text-on-line > 200"
            </TextSymbolizer>
            <TextSymbolizer
                face-name="DejaVu Sans Book"
                placement="interior"
                size="10"
                halo-radius="1"
                fill="darkorange"
                allow-overlap="true"
                dy="10"
                >
                "text-on-line"
            </TextSymbolizer>
        </Rule>
        <Rule>
            <Filter>[mapnik::geometry_type] = polygon</Filter>
            <PolygonSymbolizer />
            <TextSymbolizer
                face-name="DejaVu Sans Book"
                placement="point"
                size="10"
                halo-radius="1"
                fill="red"
                allow-overlap="true"
                largest-bbox-only="true"
                dy="-10"
                >
                "text-on-poly-largest-point"
            </TextSymbolizer>
            <TextSymbolizer
                face-name="DejaVu Sans Book"
                placement="point"
                size="10"
                halo-radius="1"
                fill="blue"
                allow-overlap="true"
                largest-bbox-only="false"
                >
                "text-on-poly-all-point"
            </TextSymbolizer>
            <TextSymbolizer
                face-name="DejaVu Sans Book"
                placement="interior"
                size="10"
                halo-radius="1"
                fill="green"
                allow-overlap="true"
                largest-bbox-only="false"
                dy="10"
                >
                "text-on-poly-all-interior"
            </TextSymbolizer>
        </Rule>
        <Rule>
            <Filter>[mapnik::geometry_type] = point</Filter>
            <MarkersSymbolizer
                ignore-placement="true"
                placement="interior"
                fill="green"
                opacity=".3"
                allow-overlap="true"
                />
            <PointSymbolizer
                ignore-placement="true"
                placement="interior"
                allow-overlap="true"
                />
        </Rule>
    </Style>

