        <Parameter name="bbox">-1, -1, 1, 1</Parameter>
    </Parameters>
    <Style name="1">
        <Rule>
            <TextSymbolizer
                placement="point"
                fill="black"
                face-name="DejaVu Sans Book"
                size="60"
                text-mode="mono"
                >
                "Test"
            </TextSymbolizer>
        </Rule>
    </Style>
    <Layer name="1">
        <StyleName>1</StyleName>
        <Datasource>
            <Parameter name="type">csv</Parameter>
            <Parameter name="inline">
                x, y
                0, 0
            </Parameter>
        </Datasource>
    </Layer>
</Map>
