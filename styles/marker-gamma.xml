<Map background-color="white">
    <Parameters>
        <Parameter name="sizes">256, 256</Parameter>
        <Parameter name="bbox">-1, -1, 1, 1</Parameter>
    </Parameters>
    <Style name="1">
        <Rule>
            <MarkersSymbolizer
                placement="point"
                file="../../data/svg/octocat.svg"
                transform="scale(0.5)"
                stroke-gamma="0.5"
                stroke-gamma-method="threshold"
                />
        </Rule>
    </Style>
    <Layer name="1">
        <StyleName>1</StyleName>
        <Datasource>
            <Parameter name="type">csv</Parameter>
            <Parameter name="inline">
                x, y
                0, 0
            </Parameter>
        </Datasource>
    </Layer>
</Map>
