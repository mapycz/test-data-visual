<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map>
<Map background-color="white" srs="+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0.0 +k=1.0 +units=m +nadgrids=@null +wktext +no_defs +over">
    <Parameters>
        <Parameter name="sizes">2048,2048</Parameter>
        <Parameter name="bbox">1213629.4,5454115.2, 1217288.3,5456223.1</Parameter>
    </Parameters>

    <Layer name="layer" srs="+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0.0 +k=1.0 +units=m +nadgrids=@null +wktext +no_defs +over">
        <StyleName>lines</StyleName>
        <Datasource>
            <Parameter name="type">csv</Parameter>
            <Parameter name="file">../../data/csv/buildings.csv</Parameter>
        </Datasource>
    </Layer>

    <Style name="lines">
        <Rule>
            <BuildingSymbolizer
                height="0"
                fill="red"
                fill-opacity="0"
                shadow-length="2"
                shadow-opacity="1"
                shadow-angle="310"
                />
        </Rule>
    </Style>

</Map>
