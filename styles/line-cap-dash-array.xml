<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map>
<Map>
    <Parameters>
        <Parameter name="sizes">256,256</Parameter>
    </Parameters>
    <Style name="lines">
        <Rule>
            <LineSymbolizer
                stroke="red"
                stroke-width="1"
                geometry-transform="translate(0, -60)"
                />
            <LineSymbolizer
                stroke="black"
                stroke-width="18"
                stroke-linecap="round"
                stroke-dasharray="0, 30"
                geometry-transform="translate(0, -60)"
                />

            <LineSymbolizer
                stroke="red"
                stroke-width="1"
                geometry-transform="translate(0, 60)"
                />
            <LineSymbolizer
                stroke="black"
                stroke-width="18"
                stroke-linecap="square"
                stroke-dasharray="0, 30"
                geometry-transform="translate(0, 60)"
                />
        </Rule>
    </Style>
    <Layer name="layer">
        <StyleName>lines</StyleName>
        <Datasource>
            <Parameter name="type">csv</Parameter>
            <Parameter name="extent">-1, -2, 11, 4</Parameter>
            <Parameter name="inline">
            wkt, placement
            "LINESTRING(0 2, 2 3, 4 2, 6 3, 8 2, 10 3)", "vertex-last"
            "LINESTRING(0 1, 10 1)", "vertex-last"
            "LINESTRING(0 0, 0 0)", "vertex-last"
            "LINESTRING(0 -1)", "vertex-last"
            </Parameter>
        </Datasource>
    </Layer>
</Map>
