<Map background-color="white" srs="+init=epsg:32630">
    <Parameters>
        <Parameter name="sizes">512,512</Parameter>
    </Parameters>

    <Style name="raster">
        <Rule>
            <RasterSymbolizer scaling="bicubic">
                <RasterColorizer>
                    <stop value="0" color="blue"/>
                    <stop value="26" color="blue"/>
                    <stop value="27" color="red"/>
                </RasterColorizer>
            </RasterSymbolizer>
        </Rule>
    </Style>

    <Layer name="dataraster" srs="+init=epsg:32630">
        <StyleName>raster</StyleName>
        <Datasource>
            <Parameter name="file">../../data/raster/dataraster.tif</Parameter>
            <Parameter name="type">gdal</Parameter>
            <Parameter name="band">1</Parameter>
        </Datasource>
    </Layer>

</Map>
